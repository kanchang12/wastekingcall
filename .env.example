# WasteKing Multi-Agent AI Configuration

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# WasteKing SMP API
WASTEKING_BASE_URL=https://wk-smp-api-dev.azurewebsites.net/
WASTEKING_ACCESS_TOKEN=your_wasteking_access_token

# Twilio SMS Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Database Configuration
DATABASE_PATH=data/conversations.db

# Agent Configuration
MAX_CONVERSATION_MEMORY=50
AGENT_TIMEOUT_SECONDS=30
ENABLE_COMPLIANCE_MONITORING=true

# Business Rules Configuration
STRICT_RULE_COMPLIANCE=true
ENABLE_EXACT_SCRIPT_VALIDATION=true
LOCK_RULE_ENFORCEMENT=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=data/logs/wasteking_ai.log
ENABLE_STRUCTURED_LOGGING=true
